{
  "name": "@lukecsamuel/expresso",
  "type": "module",
  "version": "1.2.0",
  "description": "An expression to JsonLogic transpiler",
  "files": [
    "./"
  ],
  "main": "./index.mjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./post": {
      "import": "./post.mjs",
      "require": "./post.cjs"
    }
  },
  "repository": "https://github.com/LukeCSamuel/expresso",
  "author": "Luke Caswell Samuel <7198484+LukeCSamuel@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "dev": "vitest",
    "test": "vitest run --coverage",
    "lint": "eslint . --fix",
    "send": "yarn build && cp package.json dist/package.json && npm publish ./dist"
  },
  "devDependencies": {
    "@types/json-logic-js": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.6"
  }
}
